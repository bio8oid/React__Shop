{
  "hosting": {
    "public": "./build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
  },
  "emulators": {
    "firestore": {
      "port": "8080"
    },
    "functions": {
      "port": "5001"
    },
    "database": {
      "port": "9000"
    },
    "pubsub": {
      "port": "8085"
    }
}
}